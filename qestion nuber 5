#include<stdio.h>
int main()
{
    int age,vote,rem;
    
    printf("enter your age");
    scanf("%d",&age);
    if (age>=18)
    {
        printf("you are eligible o vote");
    }
    else
    {
        printf("your are not eligible for voting");
        rem=18-age;
        printf("remanning year left:",rem);
    }
}
